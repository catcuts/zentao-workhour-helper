{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "indent": ["error", 4],
        "prettier/prettier": ["error", {}, { "usePrettierrc": true }],
        "no-console": "warn",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["warn"]
    },
    "env": {
        "node": true,
        "es6": true
    }
}